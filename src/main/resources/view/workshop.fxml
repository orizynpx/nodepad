<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="io.github.orizynpx.nodepad.controller.WorkshopController"
           fx:id="editorGraphContainer">

    <SplitPane dividerPositions="0.4" styleClass="split-pane">
        <StackPane fx:id="editorContainer" minWidth="200" styleClass="editor-pane"/>
        <StackPane fx:id="graphContainer" minWidth="200" styleClass="graph-pane-container"/>
    </SplitPane>
</StackPane>